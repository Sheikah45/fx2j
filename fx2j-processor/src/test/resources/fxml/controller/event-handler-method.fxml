<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="io.github.sheikah45.fx2j.processor.testcontroller.EventHandlerMethodController">
    <children>
        <Button onAction="#onActionWithEvent" fx:id="withEventButton"/>
        <Button onAction="#onActionWithoutEvent" fx:id="withoutEventButton"/>
        <Button onContextMenuRequested="#onContextMenuRequested" fx:id="contextButton"/>
        <Button onAction="#onActionWithEventThrow" fx:id="throwingEventButton"/>
        <Button onAction="#onActionWithoutEventThrow" fx:id="throwingNoEventButton"/>
    </children>
</AnchorPane>