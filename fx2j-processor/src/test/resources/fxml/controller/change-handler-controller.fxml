<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="io.github.sheikah45.fx2j.processor.testcontroller.ChangeHandlerController"
            fx:id="root">
    <children>
        <Button onTextChange="#onTextChange" fx:id="textChangeButton"/>
    </children>
    <styleClass onChange="#onStyleClassChange"/>
    <pseudoClassStates onChange="#onPseudoClassStatesChange"/>
    <properties onChange="#onPropertiesChange"/>
</AnchorPane>